{
  "routes": [
    {
      "route": "/*.html",
      "headers": {
        "cache-control": "must-revalidate, max-age=6000"
      }
    },
    {
      "route": "/assignments/*",
      "headers": {
        "cache-control": "must-revalidate, max-age=6000"
      }
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["*.html", "/assignments/*", "*.js", "*.css", "*.png", "*.jpg", "*.svg", "*.gif", "*.mp4", "*.wav"]
  },
  "mimeTypes": {
    ".js": "text/javascript",
    ".css": "text/css",
    ".html": "text/html",
    ".json": "application/json",
    ".svg": "image/svg+xml",
    ".png": "image/png",
    ".jpg": "image/jpeg",
    ".gif": "image/gif",
    ".mp4": "video/mp4",
    ".wav": "audio/wav"
  },
  "globalHeaders": {
    "X-Content-Type-Options": "nosniff",
    "X-Frame-Options": "SAMEORIGIN",
    "X-XSS-Protection": "1; mode=block"
  }
}