{
  "routes": [
    {
      "route": "/",
      "serve": "/index.html"
    },
    {
      "route": "/sources.html",
      "serve": "/sources.html"
    },
    {
      "route": "/assignments/*",
      "serve": "/assignments/index.html"
    },
    {
      "route": "/ai-inspiration-5degrees.html",
      "serve": "/ai-inspiration-5degrees.html"
    },
    {
      "route": "/prompt-engineering-talk.html",
      "serve": "/prompt-engineering-talk.html"
    },
    {
      "route": "/temperature-demo.html",
      "serve": "/temperature-demo.html"
    }
  ],
  "mimeTypes": {
    ".js": "text/javascript",
    ".css": "text/css",
    ".html": "text/html",
    ".json": "application/json",
    ".svg": "image/svg+xml",
    ".png": "image/png",
    ".jpg": "image/jpeg",
    ".gif": "image/gif",
    ".mp4": "video/mp4",
    ".wav": "audio/wav"
  },
  "globalHeaders": {
    "X-Content-Type-Options": "nosniff",
    "X-Frame-Options": "SAMEORIGIN",
    "X-XSS-Protection": "1; mode=block"
  }
}